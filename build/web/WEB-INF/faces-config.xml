<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>loginsuccess</from-outcome>
            <to-view-id>/ui/home/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ui/content/uploadcontent.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>addcontent</from-outcome>
            <to-view-id>/ui/content/uploadcontentfiles.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/ui/content/bulkupload_confirm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>bulkupload</from-outcome>
            <to-view-id>/ui/content/bulkupload.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>viewContentMBean</managed-bean-name>
        <managed-bean-class>net.rocg.web.mbeans.ViewContentMBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>ratingEngineBean</managed-bean-name>
        <managed-bean-class>net.rocg.web.mbeans.RatingEngineBean</managed-bean-class>
        <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>campaignManagerBean</managed-bean-name>
        <managed-bean-class>net.rocg.web.mbeans.CampaignManagerBean</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/ui/home/header.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>logout</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>html5gamebulkupload</managed-bean-name>
        <managed-bean-class>net.rocg.web.mbeans.html5gamebulkupload</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>html5gamebulkupload1</managed-bean-name>
        <managed-bean-class>net.rocg.web.mbeans.html5gamebulkupload</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
</faces-config>
